@players.map do |player|
  {
    id: player.id,
    admin: player.admin,
    name: player.name,
    email: player.email,
    address: player.address,
    position: player.position,
    age: player.age,
    level: player.level,
    games_created: player.games_created.length,
    games_attending: player.games_attending.length,
  }
end
